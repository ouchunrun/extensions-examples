{
  "name": "Desktop Capture - chrome.desktopCapture",
  "description": "Capture your desktop in popup and background with chrome.desktopCapture.",
  "version": "1.0.0.1",
  "manifest_version": 3,
  "minimum_chrome_version": "34",
  "action":
  {
    "default_title": "Desktop Capture"
  },
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "desktopCapture"
  ]
}
